    <eStructuralFeatures xsi:type="ecore:EReference" name="variable" eType="#//NameRef"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="value" eType="#//Expression"
        containment="true"/>
  </eClassifiers>
